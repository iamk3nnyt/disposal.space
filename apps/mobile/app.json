{
  "expo": {
    "name": "Disposal Space",
    "slug": "disposal-space-mobile",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "disposal-space",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.disposalspace.mobile",
      "buildNumber": "3",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false,
        "NSPhotoLibraryUsageDescription": "Allow Disposal Space to save photos and videos to your gallery.",
        "NSPhotoLibraryAddUsageDescription": "Allow Disposal Space to save downloaded files to your photo library."
      }
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#16a34a",
        "foregroundImage": "./assets/images/android-icon-foreground.png",
        "backgroundImage": "./assets/images/android-icon-background.png",
        "monochromeImage": "./assets/images/android-icon-monochrome.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.disposalspace.mobile"
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-file-system",
      [
        "expo-media-library",
        {
          "photosPermission": "Allow Disposal Space to save photos and videos to your gallery.",
          "savePhotosPermission": "Allow Disposal Space to save photos and videos to your gallery.",
          "isAccessMediaLocationEnabled": true
        }
      ],
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "backgroundColor": "#000000"
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "12226b2d-bee2-4c96-92d4-653dbc5126c3"
      }
    }
  }
}
